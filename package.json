{
  "name": "comidin-backend",
  "version": "1.0.0",
  "description": "Comidin API backend that powers the interactions between the mobile and the backoffices for merchants and administrators",
  "main": "app.js",


"scripts": {
  "start": "node app.js",
  "start:dev": "nodemon app.js",
  "dev": "concurrently \"nodemon --legacy-watch app.js\" \"bash -c 'sleep 50 && sequelize-cli db:seed:all'\"",
